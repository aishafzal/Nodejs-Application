<html>
    <head>
       
    </head>
    <body>
        <%- include('nav') -%>
        <form action = '/product/update/<%=product._id %>' method="post" enctype="multipart/form-data">
         <label>Name</label>
         <input type="text" name='name' value='<%=product.name %>'/>
         <label>Brand</label>
         <input type="text" name='brand' value='<%=product.email %>'/>
         <label>Price</label>
         <input type=Number name='price' value='<%=product.price %>'/>
         <br>
    
         <label>Update</label>
        <input type="submit" value="update" />

        </form>
        <button><a href='/product/delete/<%=product._id %>'>Delete</a></button>
    </body>
</html>